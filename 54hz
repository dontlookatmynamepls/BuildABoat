getgenv().AutoFly = true -- Đặt thành false để tắt script

local function startFlying()
    if not getgenv().AutoFly then return end

    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local hrp = character:WaitForChild("HumanoidRootPart")

    local tweenService = game:GetService("TweenService")
    local speed = 300 -- Tốc độ bay
    local flightForce = Instance.new("BodyVelocity") -- Giữ nhân vật lơ lửng

    flightForce.Velocity = Vector3.new(0, 0.1, 0) -- Giữ nhân vật trên không
    flightForce.MaxForce = Vector3.new(0, math.huge, 0) -- Chỉ tác động lực theo trục Y
    flightForce.Parent = hrp -- Gắn vào HumanoidRootPart

    -- Function để bay đến một vị trí
    local function flyTo(targetPosition)
        if not getgenv().AutoFly then return end
        local distance = (targetPosition - hrp.Position).Magnitude
        local duration = distance / speed -- Tính thời gian bay dựa trên khoảng cách và tốc độ
        
        local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Linear)
        local tween = tweenService:Create(hrp, tweenInfo, {CFrame = CFrame.new(targetPosition)})
        tween:Play()
        tween.Completed:Wait()
    end

    -- Dịch chuyển đến điểm đầu tiên
    hrp.CFrame = CFrame.new(-53.5656852722168, 100.59999084472656, 331.44317626953125)
    wait(1)

    -- Bay đến điểm thứ hai (mới)
    flyTo(Vector3.new(-53.56591796875, 100, 1753.8919677734375))
    wait(1)

    -- Dịch chuyển ngay đến điểm thứ ba
    hrp.CFrame = CFrame.new(-55.706512451171875, -358.7396240234375, 9492.3564453125)

    -- Xóa BodyVelocity khi đã đến điểm cuối
    flightForce:Destroy()
